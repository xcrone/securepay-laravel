{
    "name": "xcrone/securepay-laravel",
    "description": "SecurePay for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xcrone",
            "email": "mail@zahir.my"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Xcrone\\SecurePay\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xcrone\\SecurePay\\SecurePayServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
